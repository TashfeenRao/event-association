<div class=container>
  <div class="row">
    <div class="col-md-8">
      <h1>Event</h1>
      <h2><%= @event.title%></h2>
      <h2><%= @event.date%></h2>
    </div>
    <div class="col-md-4">
      <h1>Events List</>
      <% Event.find_each do |event| %>
      <h3><%= event.title%></h3>
      <%end%>
    </div>

  </div>
</div>